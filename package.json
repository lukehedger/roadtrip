{
  "name": "roadtrip",
  "version": "1.0.0",
  "description": "Roadtrip",
  "main": "index.js",
  "lint-staged": {
    "*.js": [
      "eslint --fix \"app/**/*.js\"",
      "prettier --write --no-semi --single-quote --trailing-comma=es5",
      "git add"
    ]
  },
  "pre-commit": {
    "run": [
      "lint:staged"
    ],
    "silent": true
  },
  "scripts": {
    "lint:staged": "lint-staged",
    "build": "webpack --config config/webpack.config.production.js --progress",
    "start": "webpack-dev-server --config ./config/webpack.config.development.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukehedger/roadtrip.git"
  },
  "keywords": [
    "roadtrip"
  ],
  "author": "Luke Hedger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lukehedger/roadtrip/issues"
  },
  "homepage": "https://github.com/lukehedger/roadtrip#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.3.1",
    "eslint": "^4.14.0",
    "eslint-config-jaak": "^1.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "html-webpack-plugin": "^2.30.1",
    "lint-staged": "^6.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.9.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "farce": "^0.2.4",
    "found": "^0.3.5",
    "immutable": "^3.8.2",
    "jaak-primitives": "^1.0.0-beta",
    "leaflet": "^1.3.1",
    "leaflet.markercluster": "^1.3.0",
    "mixpanel-browser": "^2.20.0",
    "polished": "^1.9.0",
    "prop-types": "^15.6.0",
    "raven-js": "^3.24.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-swipeable": "^4.2.1",
    "react-transition-group": "^2.2.1",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-immutablejs": "0.0.8",
    "redux-logger": "^3.0.6",
    "redux-pack": "^0.1.5",
    "reselect": "^3.0.1",
    "styled-components": "^2.4.0"
  }
